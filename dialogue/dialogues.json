{
    "dialogues" : [
        {
            "title": "The Bear",
            "path": "https://github.com/notluckycharm/alabama-dialogues/blob/ba982f71b591534b7371fbf264d95e1db3b06b16/test-dialogue/",
            "strings": [
                {
                    "wav": "pt1.wav",
                    "alabama": "Nitok ibitokachon homankan haalolikha.",
                    "english": "I heard somebody say a bear killed him.",
                    "speaker": 0,
                    "translations": [
                        "Nitok ibitokachon homankan haalolikha."
                    ]
                },
                {
                    "wav": "pt2.wav",
                    "alabama": "Sahmin ibitokachon sobaychikko?",
                    "english": "Do you not know why (a bear) killed him?",
                    "speaker": 0,
                    "translations": [
                        "Sahmin ibitokachon sobaychikko?"
                    ]
                },
                {
                    "wav": "pt3.wav",
                    "alabama": "Ah akko aatiachasobaykkobi.",
                    "english": "Ah, I don't know why it killed him.",
                    "speaker": 1,
                    "translations": [
                        "Ah akko aatiachasobaykkobi."
                    ]
                }
            ] 
        },
        {
            "title": "Where are you going?",
            "path": "https://github.com/notluckycharm/alabama-dialogues/blob/388c2f14698e3cc5842556a93792ccc9be2756cd/onthego-1/",
            "strings": [
                {
                    "wav": "pt1.wav",
                    "alabama": "Nakson aɬchiiya?",
                    "english": "Where are you going?",
                    "speaker": 0,
                    "translations": [
                        "Nakson aɬchiiya?",
                        "naksifon aɬchiiya?",
                        "Naksifa aɬchiiya?",
                        "Nakson aɬchiya?",
                        "naksifon aɬchiya?",
                        "Naksifa aɬchiya?",
                        "Nakson aɬhachiiya?",
                        "naksifon aɬhachiiya?",
                        "Naksifa aɬhachiiya?",
                        "Nakson aɬhachiya?",
                        "naksifon aɬhachiya?",
                        "Naksifa aɬhachiya?"
                    ]
                },
                {
                    "wav": "pt2.wav",
                    "alabama": "Dallas-kafon aɬiinalo.",
                    "english": "I'm going to Dallas.",
                    "speaker": 1,
                    "translations": [
                        "Dallas-kafon aɬiinalo",
                        "Dallas-kan aɬiinalo.",
                        "Dallas-kafon aɬɬalo",
                        "Dallas-kan aɬɬalo.",
                        "Dallas-kafon aɬɬalihchi",
                        "Dallas-kan aɬɬalihchi."
                    ]
                },
                {
                    "wav": "pt3.wav",
                    "alabama": "Saamikat aɬchiiya?",
                    "english": "How are you getting there?",
                    "speaker": 0,
                    "translations": [
                        "Saamikat aɬchiiya?",
                        "Saamikat aɬchiya?",
                        "Sahmikat aɬchiiya?",
                        "Sahmikat aɬchiya?",
                        "Saamikat aɬhachiiya?",
                        "Saamikat aɬhachiya?",
                        "Sahmikat aɬhachiiya?",
                        "Sahmikat aɬhachiya?"
                    ]
                },
                {
                    "wav": "pt4.wav",
                    "alabama": "Ammobilkan yaloostaɬiinalo",
                    "english": "I'm driving my own car.",
                    "speaker": 1,
                    "translations": [
                        "Ammobilkan yaloostaɬiinalo",
                        "Ammobilkan iswaliikalo",
                        "Ammobilkan swaliikalo",
